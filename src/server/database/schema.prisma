generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Card {
  id          String   @id @default(cuid())
  title       String
  url         String?
  description String?
  image_url   String?
  created_at  DateTime @default(now())
  updated_at  DateTime @updatedAt

  @@map(name: "cards")
}
