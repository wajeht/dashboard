{
  "name": "nuxt-app",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "npm run db:generate && npm run db:migrate:prod && node ./.output/server/index.mjs",
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "db:push": "npx prisma db push",
    "db:generate": "npx prisma generate",
    "db:migrate:dev": "npx prisma migrate dev",
    "db:migrate:prod": "npx prisma migrate deploy",
    "db:reset": "npx prisma migrate reset --force",
    "db:seed": "tsx ./src/server/database/seeds/seed.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "upgrade": "npx nuxi upgrade --force"
  },
  "prisma": {
    "schema": "./src/server/database/schema.prisma",
    "seed": "tsx ./src/server/database/seeds/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.7.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.4.5",
    "@faker-js/faker": "^8.3.1",
    "@formkit/auto-animate": "^0.8.1",
    "@nuxt/devtools": "latest",
    "@nuxt/image": "^1.1.0",
    "@nuxt/ui": "^2.11.0",
    "@nuxtjs/color-mode": "^3.3.2",
    "@nuxtjs/plausible": "^0.2.4",
    "@nuxtjs/robots": "^3.0.0",
    "@nuxtjs/tailwindcss": "^6.10.1",
    "@vueuse/nuxt": "^10.7.0",
    "eslint": "^8.55.0",
    "nuxt": "^3.8.2",
    "nuxt-auth-utils": "^0.0.10",
    "nuxt-vitest": "^0.11.5",
    "prisma": "^5.7.0",
    "vue": "^3.3.10",
    "vue-router": "^4.2.5"
  }
}
